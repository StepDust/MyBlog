@{
    ViewData["Title"] = "Home Page";
}

<style>
    tr th, tr td{
        text-align: center;
        padding: 7px;
    }

</style>

<table width="80%" style="text-align:center;">
    <tr>
        <th>ID</th>
        <th>UserName</th>
        <th>Password</th>
    </tr>
    @{
        if (ViewBag.list != null)
        {
            foreach (var item in ViewBag.list)
            {
                <tr>
                    <td>@item.ID</td>
                    <td>@item.UserName</td>
                    <td>@item.Password</td>
                </tr>
            }
        }
    }
</table>

